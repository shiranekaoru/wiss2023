{
    "plugins": [
        "latex2e"
    ],
    "rules": {
        "prh": {
            "rulePaths": [
                "./prh.yml"
            ]
        },
        "no-parallel-ka": true,
        "@textlint-ja/morpheme-match": {
            "dictionaryPathList": [
                "./morpheme-match-dict.js"
            ]
        },
        "preset-ja-technical-writing": {
            "ja-no-mixed-period": {
                "periodMark": "ï¼Ž"
            },
            "max-kanji-continuous-len": {
                "max": 8
            },
            "ja-no-redundant-expression": {
                "dictOptions": {
                    "dict5": {
                        "disabled": true
                    }
                }
            }
        }
    }
}
